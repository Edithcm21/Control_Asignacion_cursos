<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Modelo.AulaEntity" table="aula" schema="public" catalog="Control_Cursos">
        <id name="codigo">
            <column name="codigo" sql-type="integer"/>
        </id>
        <property name="lugar">
            <column name="lugar" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="numPupitres">
            <column name="num_pupitres" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>